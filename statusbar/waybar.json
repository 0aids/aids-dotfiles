[
  {
    "clock": {
      "format-alt": "{:%a, %d. %b  %H:%M}"
    },
    "height": 14,
    "spacing": 4,
    "hyprland/window": {
      "icon-size": 10,
      "icon": true,
      "max-length": 30
    },
    "layer": "bottom",
    "modules-left" : [
      "hyprland/workspaces",
      "hyprland/window"
    ],
    "modules-center": [
      "clock"
    ],
    "modules-right": [
      "tray",
      "wireplumber",
      "battery",
      "backlight",
      "bluetooth",
      "network"
    ],
    "position": "bottom",

    "wireplumber": {
      "format": "{icon} {volume}%",
      "tooltip": false,
      "width": "10",
      "format-muted": "<s>{icon} X%</s>",
      "format-icons": [
        "V:"
      ]
    },

    "bluetooth": {
      "format": "󰂲",
      "format-disabled": "󰂳",
      "format-connected": "󰂱",
      "tooltip-format": " Disconnected",
      "tooltip-format-connected": "{device_enumerate}",
      "tooltip-format-enumerate-connected": " {device_alias} 󰂄{device_battery_percentage}%",
      "tooltip": true
    },
    "tray": {
      "icon-size": 10,
      "spacing": 4
    },


    "backlight": {
      "format": "{icon}",
      "tooltip-format": "Brightness: {percent}%",
      "format-icons": ["","","","","","","","","","","","","","",""]
    },
    "battery": {
      "interval": 5,
      "align": 0,
      "rotate": 0,
      "width": 10,
      "bat": "BAT0",
      "adapter": "ACAD",
      "full-at": 100,
      "design-capacity": false,
      "states": {
        "good": 95,
        "warning": 30,
        "critical": 15
      },
      "format": "{icon} {capacity}%",
      "format-charging": "  {capacity}%",
      "format-plugged": " 󱘖 {capacity}%",
      "format-full": " {icon} 100%",
      "format-alt": " {icon} {time}",
      "format-icons": [
        "B:"
      ],
      "format-time": "{H}h {M}min",
      "tooltip": true,
      "tooltip-format": "{timeTo} {power}w"
    },

    
    "clock": {
      "format": "{:%d/%m %a %H:%M}",
      "tooltip-format": "<tt><small>{calendar}</small></tt>"
    },
    "cpu": {
      "interval": 1,
      "min-length": 5,
      "format": "{icon0}{icon1}{icon2}{icon3} {usage:>2}% 󰍛",
      "format-icons": [
        "▁", "▂", "▃", "▄", "▅", "▆", "▇", "█"
      ],
      "on-click-right": "gnome-system-monitor"
    },
    "network": {
      "format": "{ifname} ",
      "format-wifi": "{icon} ",
      "format-ethernet": "󰌘 ",
      "format-disconnected": "󰈂 ",
      "tooltip-format": "{ipaddr}  {bandwidthUpBits}  {bandwidthDownBits}",
      "format-linked": "󰈁 {ifname} (No IP) ",
      "tooltip-format-wifi": "{essid} {icon} {signalStrength}%",
      "tooltip-format-ethernet": "{ifname} 󰌘",
      "tooltip-format-disconnected": "󰈂 Disconnected",
      "max-length": 30,
      "format-icons": [
        "󰤯", "󰤟", "󰤢", "󰤥", "󰤨"
      ]
    }


  }
]

